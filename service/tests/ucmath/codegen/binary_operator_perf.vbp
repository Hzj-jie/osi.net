
##DEFAULT TEST_SIZE 1073741824
##DEFAULT RND rnd_int()
Imports osi.root.connector
Imports osi.root.utt

Public Class ##CLASS_TYPE##_perf
    Inherits performance_case_wrapper

    Public Sub New()
        MyBase.New(New ##CLASS_TYPE##_case())
    End Sub

    Private Class ##CLASS_TYPE##_case
        Inherits [case]

        Private ReadOnly r1 As ##TYPE##
        Private ReadOnly r2 As ##TYPE##

        Public Sub New()
            r1 = ##RND##
            r2 = 0
            While r2 = 0
                r2 = ##RND##
            End While
			Dim r As ##TYPE## = 0
			##BINARY_OPERATOR##
        End Sub

        Public Overrides Function run() As Boolean
            For i As Int64 = 0 To ##TEST_SIZE## - 1
                Dim r As ##TYPE## = 0
				##BINARY_OPERATOR##
            Next
            Return True
        End Function
    End Class
End Class
